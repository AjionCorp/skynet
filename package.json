{
  "name": "skynet-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "lint:sh": "shellcheck -S warning scripts/*.sh scripts/agents/*.sh scripts/notify/*.sh",
    "lint:ts": "eslint packages/*/src/",
    "dev:admin": "pnpm --filter admin dev",
    "test": "pnpm -r --filter '@ajioncorp/*' test",
    "test:e2e": "pnpm --filter admin test:e2e",
    "test:e2e:cli": "bash tests/e2e/init-smoke.test.sh",
    "test:e2e:cli-commands": "bash tests/e2e/cli-commands.test.sh"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "typescript-eslint": "^8.56.0"
  }
}
