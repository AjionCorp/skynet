<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.skynet.SKYNET_PROJECT_NAME.project-driver</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>SKYNET_SCRIPTS_DIR/project-driver.sh</string>
  </array>
  <key>EnvironmentVariables</key>
  <dict>
    <key>SKYNET_DEV_DIR</key>
    <string>SKYNET_DEV_DIR</string>
  </dict>
  <key>StartCalendarInterval</key>
  <array>
    <dict>
      <key>Hour</key>
      <integer>8</integer>
      <key>Minute</key>
      <integer>0</integer>
    </dict>
    <dict>
      <key>Hour</key>
      <integer>20</integer>
      <key>Minute</key>
      <integer>0</integer>
    </dict>
  </array>
  <key>StandardOutPath</key>
  <string>SKYNET_DEV_DIR/scripts/project-driver.log</string>
  <key>StandardErrorPath</key>
  <string>SKYNET_DEV_DIR/scripts/project-driver.log</string>
  <key>RunAtLoad</key>
  <false/>
</dict>
</plist>
